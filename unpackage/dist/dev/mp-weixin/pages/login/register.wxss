page {
  background-color: #fff;
}
.register .container {
  padding: 30rpx 100rpx 60rpx;
}
.register .container .logo {
  width: 380rpx;
  height: 380rpx;
}
.register .container .form {
  margin-top: 100rpx;
  width: 100%;
}
.register .container .form .cu-form-group {
  background: #fff;
  margin-top: 30rpx;
  border-radius: 100rpx;
  padding: 20rpx 40rpx;
  font-size: 36rpx;
  color: #8799a3;
  border: 1rpx solid #DDDDDD;
}
.register .container .form .cu-form-group input {
  height: 50rpx;
  line-height: 50rpx;
}
.register .container .form .cu-form-group.code {
  padding: 20rpx 0 20rpx 40rpx;
}
.register .container .form .cu-form-group.code .sendCodeBtn {
  padding-left: 20rpx;
  padding-right: 40rpx;
  border-left: 1px solid #cdcdcd;
  height: 50rpx;
  border-radius: 0;
  line-height: 24px;
  background: transparent;
  font-size: 16px;
  /* color: #333; */
}
.register .container .form .zai-btn {
  background: #ffcb57;
  color: #fff;
  border: 0;
  border-radius: 100rpx;
  font-size: 36rpx;
  margin-top: 60rpx;
}
.register .container .form .zai-label {
  text-align: center;
  font-size: 30rpx;
  color: #8799a3;
}
.placeholde {
  color: #8799a3;
}

