.enroll .cu-list .cu-item {
  padding: 30rpx;
}
.enroll .cu-list .cu-item .content {
  width: 100%;
}
.enroll .cu-list .cu-item .content .title {
  width: 100%;
}
.enroll .cu-list .cu-item .content .title .tag {
  letter-spacing: 3rpx;
  font-size: 40rpx;
  border-right: 1rpx solid #C1C1C1;
  line-height: 1;
}
.enroll .cu-list .cu-item .content .title .enroll-title {
  font-weight: 500;
}
.enroll .cu-list .cu-item .content .imgbox .imgbox-img {
  width: 100%;
}
.enroll .cu-list .cu-item .content .bot .bot-avatar {
  width: 28rpx;
  height: 28rpx;
  border-radius: 100%;
  border: 1rpx solid #fff;
}
.enroll .cu-list .cu-item .content .bot .bot-avatar.stack {
  margin-right: -10rpx;
}

