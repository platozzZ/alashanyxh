page, page {
  width: 100%;
  height: 100vh;
}
.sd .bg {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: -1;
}
.sd .match-status {
  width: 100%;
}
.sd .match-status .match-status-fixed {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
}
.sd .container {
  padding: 60rpx 0;
}
.sd .container .sd-logo {
  width: 444rpx;
  height: 470rpx;
}
.sd .container .sd-notice {
  font-size: 36rpx;
  color: #F6FBFC;
}
.sd .sd-list .sd-item {
  width: 560rpx;
  height: 120rpx;
  background: rgba(255, 255, 255, 0.4);
  border-radius: 4px;
}
.sd .p-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.8);
  z-index: 9999;
  color: #fff;
  -webkit-transition: all 0.2s linear;
  transition: all 0.2s linear;
  -webkit-transform: translateY(100vh);
          transform: translateY(100vh);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.sd .p-mask.maskAnimate {
  -webkit-transform: translateY(0);
          transform: translateY(0);
}
.sd .p-mask .p-mask-container {
  padding: 80rpx 50rpx 60rpx;
  font-size: 28rpx;
  width: 100%;
  position: relative;
  top: 70rpx;
  left: 0;
  height: calc(100% - 300rpx);
  overflow-y: auto;
  padding-bottom: 30rpx;
  box-sizing: border-box;
}
.sd .p-mask .p-mask-container .p-mask-title {
  font-size: 36rpx;
}
.sd .p-mask .p-mask-container .p-mask-content {
  padding: 30rpx 0;
  margin: 30rpx 0;
  color: #fff;
}
.sd .p-mask .p-mask-container .p-mask-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 20rpx 0;
}
.sd .p-mask .p-mask-container .p-mask-bot {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.sd .p-mask .p-mask-container .p-mask-close {
  position: fixed;
  left: 50%;
  bottom: 150rpx;
  width: 80rpx;
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border: 1rpx solid #fff;
  border-radius: 100%;
  font-size: 40rpx;
  -webkit-transform: translateX(-40rpx);
          transform: translateX(-40rpx);
}
.sd .maskAnimate {
  -webkit-animation: myMask 0.5s 1;
          animation: myMask 0.5s 1;
  -webkit-animation-timing-function: ease;
          animation-timing-function: ease;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
}
@-webkit-keyframes myMask {
from {
    bottom: 100vh;
}
to {
    bottom: 0;
}
}
@keyframes myMask {
from {
    bottom: 100vh;
}
to {
    bottom: 0;
}
}

